{
  "routes": [
    {
      "src": "^/$",
      "dest": "/hero-landing.html",
      "headers": {
        "Cache-Control": "public, max-age=86400, s-maxage=86400"
      }
    },
    {
      "src": "^/app/(.*)",
      "dest": "/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/hero-landing.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/(.*\\.(avif|webp|jpg|jpeg|png|gif|svg))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "src/app/**/*.tsx": {
      "maxDuration": 10
    }
  }
}
