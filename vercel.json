{
  "rewrites": [
    {
      "source": "/",
      "destination": "/hero-landing.html"
    },
    {
      "source": "/app/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/hero-landing.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }

  ],
  "functions": {
    "src/app/**/*.tsx": {
      "maxDuration": 10
    }
  }
}
